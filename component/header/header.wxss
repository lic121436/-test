.navgation-wrapper {
  width: 100%;
}

.navgation-wrapper .navgation-placeholder {
  width: 100%;
  height: 128rpx;
}

.navgation-wrapper .navigation {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  z-index: 100;
  top: 0;
  left: 0;
  width: 100%;
  height: 88rpx;
  padding-top: 40rpx;
}

.navgation-wrapper .navigation .action-wrapper {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  width: 120rpx;
  padding-left: 20rpx;
  font-size: 26rpx;
}

.navgation-wrapper .navigation .action-wrapper button {
  padding: 0;
  margin: 0;
  width: auto;
  height: auto;
  background-color: transparent;
  border-radius: 0;
}

.navgation-wrapper .navigation .action-wrapper button::after {
  border: none;
}

.navgation-wrapper .navigation .action-wrapper .iconfont {
  margin-right: 20rpx;
  font-size: 36rpx;
  color: #141414;
}

.navgation-wrapper .navigation .action-wrapper .backgo {
  margin-right: 20rpx;
}

.navgation-wrapper .navigation .action-wrapper .home .iconfont {
  font-size: 45rpx;
}

.navgation-wrapper .navigation .title {
  width: 470rpx;
  height: 44rpx;
  line-height: 44rpx;
  text-align: center;
  font-size: 26rpx;
  color: #141414;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}

.navgation-wrapper.isx .navgation-placeholder {
  height: 196rpx;
}

.navgation-wrapper.isx .navigation {
  padding-top: 88rpx;
}

.navgation-wrapper.android .navgation-placeholder {
  height: 144rpx;
}

.navgation-wrapper.android .navigation {
  height: 96rpx;
  padding-top: 68rpx;
}

.navgation-wrapper.android .navigation .title {
  text-align: left;
}

